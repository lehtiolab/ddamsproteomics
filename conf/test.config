process {
  cpus = 2
  memory = 4.GB
  time = 0.5.h

  withName: msgfPlus {
    cpus = 2
    time = { check_max( 0.5.h * task.attempt, 'time' ) }
    memory = 4.GB
  }
  withName: sage {
    cpus = 2
    time = { check_max( 0.5.h * task.attempt, 'time' ) }
    memory = 15.8.GB
  }
}

docker {
  enabled = true
  fixOwnership = true
}
